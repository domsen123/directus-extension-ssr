{
  "name": "directus-extension-webdav",
  "type": "module",
  "version": "0.0.1",
  "author": "Dominic Marx",
  "license": "ISC",
  "homepage": "https://github.com/domsen123/directus-extension-ssr",
  "repository": {
    "type": "git",
    "url": "https://github.com/domsen123/directus-extension-ssr"
  },
  "bugs": {
    "url": "https://github.com/domsen123/directus-extension-ssr/issues"
  },
  "keywords": [
    "directus",
    "extension",
    "webdav"
  ],
  "exports": {
    "./package.json": "./package.json"
  },
  "main": "dist/api.cjs",
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean:build": "rm -rf dist && tsup"
  },
  "directus:extension": {
    "type": "bundle",
    "path": {
      "api": "dist/api.cjs",
      "app": "dist/app.cjs"
    },
    "entries": [
      {
        "name": "directus-extension-webdav",
        "type": "hook",
        "source": ""
      }
    ],
    "host": "^10.4.2"
  },
  "peerDependencies": {
    "@directus/sdk": "^12.0.1",
    "express": "^4.18.2"
  },
  "dependencies": {
    "@directus/extensions-sdk": "^10.1.11",
    "directus": "^10.6.3",
    "webdav-server": "^2.6.2"
  },
  "devDependencies": {
    "@directus/errors": "^0.0.2",
    "@directus/sdk": "^12.0.1",
    "@directus/types": "^10.1.6",
    "@types/express": "^4.17.18",
    "@types/node": "^20.8.2",
    "express": "^4.18.2",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
